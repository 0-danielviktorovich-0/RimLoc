{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HealthReport",
  "type": "object",
  "required": [
    "checked",
    "issues"
  ],
  "properties": {
    "checked": {
      "type": "integer",
      "format": "uint",
      "minimum": 0.0
    },
    "issues": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/HealthIssue"
      }
    }
  },
  "definitions": {
    "HealthIssue": {
      "type": "object",
      "required": [
        "category",
        "error",
        "path"
      ],
      "properties": {
        "category": {
          "type": "string"
        },
        "error": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      }
    }
  }
}